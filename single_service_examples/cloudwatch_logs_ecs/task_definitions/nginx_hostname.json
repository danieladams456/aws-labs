[
  {
    "name": "nginxhostname",
    "image": "danieladams456/nginxhostname",
    "cpu": 128,
    "memoryReservation": 128,
    "memory": 256,
    "portMappings": [
      {
        "containerPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "us-east-1",
        "awslogs-group": "container-test",
        "awslogs-stream-prefix": "nginx_hostname"
      }
    }
  }
]
