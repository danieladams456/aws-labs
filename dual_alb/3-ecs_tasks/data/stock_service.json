[
  {
    "name": "stock",
    "image": "danieladams456/service-discovery-stubs:stock",
    "memory": 128,
    "memoryReservation": 64,
    "cpu": 128,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 5000,
        "protocol": "tcp"
      }
    ],
    "environment": [
      { "name": "BASE_URL", "value": "http://services.internal.dadams.io" }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "us-east-1",
        "awslogs-group": "/ecs/stock_service"
      }
    }
  }
]
